# .env.example - 环境变量示例文件
# 将此文件复制为 .env 并填入你的实际值。

# --- 核心配置 ---
# 你的 Discord 机器人令牌
DISCORD_TOKEN="YOUR_DISCORD_TOKEN_HERE"

# (可选) 用于在开发时快速同步斜杠命令的服务器（公会）ID
# 如果留空，命令将进行全局同步（可能需要长达一小时才能生效）
GUILD_ID="YOUR_DEVELOPMENT_GUILD_ID_HERE"

# --- 新增：权限控制 ---
# 拥有此命令访问权限的开发者用户ID，多个ID请用逗号分隔
DEVELOPER_USER_IDS="YOUR_USER_ID_1,YOUR_USER_ID_2"

# 拥有此命令访问权限的身份组ID，多个ID请用逗号分隔
ADMIN_ROLE_IDS="YOUR_ROLE_ID_1,YOUR_ROLE_ID_2"

# --- Gemini AI 配置 ---
# Google Gemini API 密钥 (用于AI对话功能)
# 多个密钥请直接换行，并用英文双引号 "..." 将所有密钥包裹起来
GOOGLE_API_KEYS_LIST="
YOUR_GEMINI_API_KEY_1
YOUR_GEMINI_API_KEY_2
"

# (可选) 自定义 Gemini API 端点，例如使用代理或 Cloudflare Worker
# GEMINI_API_BASE_URL="https://your-worker.workers.dev/gemini"

# --- OpenAI 配置 ---
# OpenAI API 密钥 (可选，用于核心对话功能)
OPENAI_API_KEY="YOUR_OPENAI_API_KEY_HERE"

# (可选) 自定义 OpenAI API 端点，例如使用代理或兼容服务
# OPENAI_API_BASE_URL="https://api.openai.com/v1"

# (可选) 指定 OpenAI 模型，默认为 gpt-4o
# 可选值: gpt-4o, gpt-4o-mini, gpt-4-turbo, gpt-3.5-turbo 等
# OPENAI_MODEL="gpt-4o"

# 是否使用 OpenAI 进行核心对话 (true/false)
# 设置为 true 时，核心对话功能将使用 OpenAI
# 设置为 false 时，核心对话功能将使用 Gemini
# 注意：嵌入向量和安全评分始终使用 Gemini
USE_OPENAI_FOR_CHAT=false

# 配置WebUI网页 for example 8001
CONFIG_WEB_PORT="8001"
#多个请直接换行
WEBUI_ADMIN_TOKEN="
Very_Very_Strong_Token_1
Very_Very_Strong_Token_2
"

# --- 功能开关 ---
# 是否启用聊天功能 (True/False)
CHAT_ENABLED=True

# 是否启用RAG查询重写功能 (1 for enabled, 0 for disabled)
RAG_QUERY_REWRITING_ENABLED=1
